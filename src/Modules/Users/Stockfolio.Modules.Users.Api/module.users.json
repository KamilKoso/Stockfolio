{
  "users": {
    "module": {
      "name": "Users",
      "enabled": true
    },
    "registration": {
      "enabled": true,
      "invalidEmailProviders": [
        "discard",
        "dispostable",
        "mailinator"
      ]
    },
    "passwordStrengthPolicy": {
      "minLength": "6",
      "maxLength": "50",
      "requireSpecialCharacter": true,
      "requireUppercaseCharacter": true,
      "requireNumber": true
    }
  }
}